<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <Version>0.1.1</Version>
    </PropertyGroup>

    <ItemGroup>
      <None Remove="registry.json" />
    </ItemGroup>

    <ItemGroup>
      <Content Include="..\..\registry.json" Link="registry.json">
        <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      </Content>
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
      <PackageReference Include="NuGet.Packaging" Version="5.1.0" />
      <PackageReference Include="NuGet.Protocol" Version="5.1.0" />
      <PackageReference Include="Microsoft.AspNetCore" Version="2.2.0" /> 
      <PackageReference Include="Microsoft.Extensions.Caching.Memory" Version="2.2.0" /> 
      <PackageReference Include="NuGet.Resolver" Version="5.1.0" /> 
      <PackageReference Include="NUglify" Version="1.5.13" /> 
      <PackageReference Include="Scriban" Version="2.1.0" /> 
      <PackageReference Include="SharpZipLib" Version="1.1.0" />
    </ItemGroup>
</Project>
